    <style>
        /* About Us Specific Styles */
        .about-hero {
            background: linear-gradient(135deg, #1e3a5f 0%, #2c5f8d 100%);
            color: white;
            padding: 120px 0 80px;
            text-align: center;
        }

        .about-hero h1 {
            font-size: 3.5rem;
            margin-bottom: 1.5rem;
            font-weight: 700;
        }

        .about-hero p {
            font-size: 1.3rem;
            max-width: 800px;
            margin: 0 auto;
            line-height: 1.8;
            opacity: 0.95;
        }

        .about-section {
            padding: 80px 0;
        }

        .about-section:nth-child(even) {
            background-color: #f8f9fa;
        }

        .section-title {
            text-align: center;
            font-size: 2.5rem;
            margin-bottom: 1rem;
            color: #1e3a5f;
            font-weight: 700;
        }

        .section-subtitle {
            text-align: center;
            font-size: 1.2rem;
            color: #666;
            margin-bottom: 60px;
            max-width: 700px;
            margin-left: auto;
            margin-right: auto;
        }

        /* Company Story */
        .company-story {
            max-width: 1000px;
            margin: 0 auto;
            font-size: 1.1rem;
            line-height: 1.9;
            color: #333;
        }

        .company-story p {
            margin-bottom: 1.5rem;
        }

        .company-story strong {
            color: #1e3a5f;
        }

        /* Founders Section */
        .founders-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 60px;
            max-width: 1200px;
            margin: 0 auto;
        }

        .founder-card {
            background: white;
            border-radius: 20px;
            padding: 40px;
            box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

        .founder-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.15);
        }

        .founder-header {
            display: flex;
            align-items: center;
            margin-bottom: 30px;
        }

        .founder-avatar {
            width: 120px;
            height: 120px;
            border-radius: 50%;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 3rem;
            color: white;
            font-weight: 700;
            margin-right: 30px;
        }

        .founder-info h3 {
            font-size: 1.8rem;
            margin-bottom: 0.5rem;
            color: #1e3a5f;
        }

        .founder-info .title {
            font-size: 1.1rem;
            color: #667eea;
            font-weight: 600;
            margin-bottom: 0.3rem;
        }

        .founder-info .ownership {
            font-size: 0.95rem;
            color: #999;
        }

        .founder-bio {
            font-size: 1.05rem;
            line-height: 1.8;
            color: #555;
        }

        /* Ownership Structure */
        .ownership-diagram {
            max-width: 800px;
            margin: 0 auto;
            background: white;
            padding: 60px;
            border-radius: 20px;
            box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);
        }

        .ownership-level {
            text-align: center;
            margin-bottom: 40px;
        }

        .ownership-box {
            display: inline-block;
            padding: 20px 40px;
            border: 3px solid #1e3a5f;
            border-radius: 15px;
            font-size: 1.2rem;
            font-weight: 600;
            color: #1e3a5f;
            background: white;
        }

        .ownership-arrow {
            text-align: center;
            font-size: 2rem;
            color: #1e3a5f;
            margin: 20px 0;
        }

        .ownership-shareholders {
            display: flex;
            justify-content: center;
            gap: 40px;
            flex-wrap: wrap;
        }

        .shareholder {
            text-align: center;
        }

        .shareholder-box {
            padding: 20px 30px;
            border: 3px solid #667eea;
            border-radius: 15px;
            font-size: 1.1rem;
            font-weight: 600;
            color: #667eea;
            background: white;
            margin-bottom: 10px;
        }

        .shareholder-percentage {
            font-size: 1.3rem;
            font-weight: 700;
            color: #1e3a5f;
        }

        /* Team Section */
        .team-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 40px;
            max-width: 1400px;
            margin: 0 auto;
        }

        .team-member {
            background: white;
            border-radius: 20px;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

        .team-member:hover {
            transform: translateY(-10px);
            box-shadow: 0 20px 50px rgba(0, 0, 0, 0.15);
        }

        .team-member-avatar {
            width: 100%;
            height: 300px;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 5rem;
            color: white;
            font-weight: 700;
        }

        .team-member-info {
            padding: 30px;
        }

        .team-member-info h3 {
            font-size: 1.5rem;
            margin-bottom: 0.5rem;
            color: #1e3a5f;
        }

        .team-member-info .role {
            font-size: 1.1rem;
            color: #667eea;
            font-weight: 600;
            margin-bottom: 1rem;
        }

        .team-member-info p {
            font-size: 1rem;
            line-height: 1.7;
            color: #666;
        }

        /* Values Section */
        .values-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
            gap: 40px;
            max-width: 1400px;
            margin: 0 auto;
        }

        .value-card {
            background: white;
            padding: 40px;
            border-radius: 20px;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
            text-align: center;
            transition: transform 0.3s ease;
        }

        .value-card:hover {
            transform: translateY(-10px);
        }

        .value-icon {
            width: 80px;
            height: 80px;
            margin: 0 auto 20px;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 2.5rem;
            color: white;
        }

        .value-card h3 {
            font-size: 1.5rem;
            margin-bottom: 1rem;
            color: #1e3a5f;
        }

        .value-card p {
            font-size: 1.05rem;
            line-height: 1.7;
            color: #666;
        }

        /* Responsive */
        @media (max-width: 768px) {
            .about-hero h1 {
                font-size: 2.5rem;
            }

            .founders-grid {
                grid-template-columns: 1fr;
                gap: 40px;
            }

            .ownership-shareholders {
                flex-direction: column;
                gap: 20px;
            }

            .team-grid {
                grid-template-columns: 1fr;
            }
        }
    </style>
</head>
